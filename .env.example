# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# OpenAI API Key (for AI coaches and food scanning)
EXPO_PUBLIC_OPENAI_API_KEY=sk-your-openai-key-here

# Stripe Configuration (for subscriptions)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key-here
EXPO_PUBLIC_STRIPE_PREMIUM_MONTHLY_PRICE_ID=price_monthly_id_here
EXPO_PUBLIC_STRIPE_PREMIUM_YEARLY_PRICE_ID=price_yearly_id_here

# Sentry (Optional - for error tracking)
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn-here

# Development Settings
EXPO_PUBLIC_BYPASS_AUTH=false
EXPO_PUBLIC_APP_ENV=development
